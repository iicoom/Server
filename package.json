{
  "name": "Server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon index.js",
    "dev": "better-npm-run start-dev",
    "unit-test": "better-npm-run start-test",
    "functional": "better-npm-run start-functional"
  },
  "betterScripts": {
    "start-test": {
      "command": "mocha test",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "start-dev": {
      "command": "nodemon index.js",
      "env": {
        "PORT": 3004,
        "NODE_ENV": "development",
        "DEBUG": "koa-redis,koa-generic-session"
      }
    },
    "start-functional": {
      "command": "node index.js",
      "env": {
        "PORT": 3004,
        "NODE_ENV": "functional",
        "DEBUG": "koa-redis,koa-generic-session"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iicoom/Server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iicoom/Server/issues"
  },
  "homepage": "https://github.com/iicoom/Server#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "koa-session": "^5.4.0",
    "koa-validate": "1.0.6",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.5",
    "sha1": "^1.1.1",
    "speakeasy": "^2.0.0",
    "uuid": "^3.1.0",
    "xss": "^0.3.3",
    "import-dir": "0.0.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "nodemon": "^1.12.1"
  }
}
